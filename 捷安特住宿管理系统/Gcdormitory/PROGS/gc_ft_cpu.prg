**
FUNCTION GC_FT_Cpu
PARAMETER FT1, FT2, SK1, SK2, KC
IF (FT1<=SK1 .AND. FT1<=SK2 .AND. FT2<=SK1 .AND. FT2<=SK2) .OR. (FT1>=SK1 .AND. FT1>=SK2 .AND. FT2>=SK1 .AND. FT2>=SK2)
MRET = 0
ELSE
DO CASE
CASE FT1>=SK1 .AND. FT1<=SK2 .AND. FT2>=SK1 .AND. FT2<=SK2
MRET = KC
CASE FT1>=SK1 .AND. FT1<=SK2 .AND. FT2>SK2
MRET = SK2-FT1
CASE FT1<SK1 .AND. FT2>=SK1 .AND. FT2<=SK2
MRET = FT2-SK1
OTHERWISE
MRET = 0
ENDCASE
ENDIF
RETURN MRET
ENDFUNC
**
