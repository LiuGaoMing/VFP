PARAMETER IA_POINTER
COUNT TO CNTREC
IF CNTREC=0
APPEND BLANK
RETURN
ELSE
GOTO IA_POINTER
ENDIF
IF EOF()
APPEND BLANK
REPLACE YGLB WITH YGLB.LBBH
ELSE
SKIP
IF EOF()
APPEND BLANK
REPLACE YGLB WITH YGLB.LBBH
ELSE
APPEND BLANK
REPLACE YGLB WITH YGLB.LBBH
SCATTER TO IA_BLANK 
DO WHILE  .NOT. BOF()
SKIP -1
SCATTER TO IA_RECORD 
SKIP
GATHER FROM IA_RECORD
SKIP -1
IF RECNO()=IA_POINTER
